algoritmo "Exe010403"

   // 1 - fazer um algoritmo para somar dois números inteiros
   
   var
     Num, Num2, Soma:inteiro

   inicio
     escreva("Informe o primeiro número: ")
     leia(Num)
     escreva("Informe o segundo número: ")
     leia(Num2)
     Soma <- Num - Num2
     escreva("O resultado da soma: ",Soma)
   

fimalgoritmo
